    <div class="listing-container">
        <div class="filter-container surface-secondary">
            <label class="dm-sans-h1 filter-heading" for="location">Search for a location</label>
            <div class="search-container">
                <input class="dm-sans-body search-bar surface-secondary with-shadow-reverse" id="location" placeholder="Search for a location" />
