                        <div class="property-card_highlight-details">
                            <div class="property-card_highlight-detail">
                                <p class="dm-sans-h3">£5,350</p>
                                <p class="dm-sans-body">per month</p>
                            </div>
                            <div class="property-card_highlight-detail">
                                <p class="dm-sans-h3">0.21</p>
                                <p class="dm-sans-body">km</p>
                            </div>
                        </div>
                        <div class="property-card_details">
                            <div class="dm-sans-h3 property-card_details-header">3 Bed Flat, Top Floor, WC2N</div>
